{
  "name": "spicy-wod-2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "wrangler deploy",
    "dev": "react-router dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler dev",
    "typecheck": "react-router typegen && tsc",
    "typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio --config drizzle.studio.config.ts",
    "db:seed": "wrangler d1 execute dev-spicy-db --local --file ./seed.sql",
    "db:list": "wrangler d1 migrations list dev-spicy-db --local",
    "db:list:preview": "wrangler d1 migrations list dev-spicy-db --env preview",
    "db:list:production": "wrangler d1 migrations list dev-spicy-db --env production",
    "db:apply": "drizzle-kit generate && wrangler d1 migrations apply dev-spicy-db --local",
    "db:apply:preview": "wrangler d1 migrations apply dev-spicy-db --env preview",
    "db:apply:production": "wrangler d1 migrations apply dev-spicy-db --env production"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@react-router/cloudflare": "^7.1.1",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.3",
    "isbot": "^4.1.0",
    "lucide-react": "^0.474.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "@react-router/dev": "^7.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "better-sqlite3": "^11.8.1",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "shadcn-ui": "^0.9.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.99.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.15.2+sha512.93e57b0126f0df74ce6bff29680394c0ba54ec47246b9cf321f0121d8d9bb03f750a705f24edc3c1180853afd7c2c3b94196d0a3d53d3e069d9e2793ef11f321"
